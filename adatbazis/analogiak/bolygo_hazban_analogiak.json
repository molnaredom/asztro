{
  "nap": {
    "1": {
      "általános": [
        "nagyon fontos, hogy legyek valaki",
        "kreativitás",
        "vezető szerep",
        "nehezen viselik az alárendeltséget",
        "nehezen viselik a korlátokat",
        "fontos az önmegvalósítás",
        "lehetőséget ada  médiára, tanításra",
        "előtérbe kerül a teremtésnél a gyerek, a vállalkozás és más alkotói pálya",
        "fontos az apaminta",
        "mások felnéznek rá, tisztelik, megbcsülik",
        "társadalmilag elismert pozíciók",
        "magas beosztás",
        "olyan hivatás választása ahol nem megkérdőjelezhető",
        "gerincproblémák, szívproblémák, agyproblémák, szemproblémák, vagy gyógyíthatatlan betegségek"
      ],
      "férfi": [
        "legyen önmaga a nap",
        "fontos a teremtés analógiája",
        "ha problémája van nem tud nap lenni"
      ],
      "nő": [
        "férfin keresztül kellene megélnie",
        "ha problémája van: nincs kire felnéznie"
      ]
    },
    "2": {
      "általános": [

        "anyagiak jelennek meg elsősorban",
        "olyan plyafutás, aho anyagiakkal foglalkozik",
        "bróker, bankár, földekkel való foglalkozás",
        "Mások tudják nappá tenni, egyedül nem tud",
        "sikeres anyagi befektetések, előrejutás",
        "jó anyagi háttér",
        "fontos milyen a külső",
        "törődik a testével",
        "törődik azzal milyen hatással vana  világra",
        "az élvezetek nagy hangsúlyt kapnak",
        "szeretik a jó dolgokat a luxust"
      ],
      "férfi": [
        "akkor teljesül ki, amikor az anyagiakon keresztül próbál kiteljesedni"
      ],
      "nő": [
        "olyan pár való neki, akire anyagiak miatt fel tud nézni",
        "nem szabad olyan pasit választani aki nem teremti elő az anyagiakat, mert a kapcsolat tönkremegy"
      ]
    },
    "3": {
      "általános": [
        "a környezetében felnéznek rá",
        "fontos neki mi veszi körül: lakás, barátok..",
        "fontos milyen iskolába jár",
        "megjelenhet a média életében",
        "színészet: világ tanításának szimbóluma",
        "fontos lehet az utazás",
        "tanítói írói képességek -> sikeresség",
        "mutathat fiú testvért",
        "mutathatja , hogy felnézel a testvéredre"
      ]
    },
    "4": {
      "általános": [
        "felnéz az apjára, tiszteli az apját",
        "az apaminta meghatározó, gyermekkorában fontos volt az apa",
        "fontos az otthon a család",
        "nagy múltú család, nagy birtokkal rendelkező család",
        "sok ingatlannal vaó rendelkezés",
        "olyan hivatás, ahol ingatlannal foglalkozik az illető: építész, beruházó, mérnök, ingatlanügynök, ingatlankzvetítő",
        "fontos neki, hogy legyen saját ingatlana",
        "egészségügyi problémák azért alakulhatnak ki, hogy az otthonba kényszerítsen"
      ],
      "nő": [
        "általában egyedül marad élete végén"
      ]
    },
    "5": {
      "általános": [
        "fontos hogy én legyek valaki",
        "fontos az önmegvalósítás",
        "az én kifejezés nagyon fontos",
        "mutathat siekres vállalkozást",
        "szereti magát megmutatni a világnak",
        "nagoyn erős sikeréhesség, sikerorientáltság",
        "felerősödik az élet szeretete, az élet örömteli",
        "fontos a testiség a szexualitás",
        "erős ösztönösség: erős vágyak, erős egó",
        "gazdag apa",
        "nagyon gyakran főleg régen a fiú elsőgyermekre utalt",
        "nagyon fontos a gyerek",
        "sikeres lesz a gyermeke"
      ],
      "nő": [
        "a gyerekáltal nappá válik: egyedül marad",
        "nem tud a gyermekről lemondani",
        "a férfiban kellene a napot megélnie"
      ]
    },
    "6": {
      "általános": [
        "saját vállalkozás indítása ebben dolgozik",
        "gyakori a gerincsérvvel való megélés",
        "szolgálat és alárendeltség háza",
        "nem tud alárendelődni",
        "azt hiszi a munkán keresztül kell kiteljesednie(munkamániások)",
        "szeretne vezető beosztásban dolgozni",
        "betegségek: szív, agy, gerinc, szem, idegrendszer , életmód: gyógyítható betegségek",
        "állatokon keresztül nappá válni: pl állattenyésztés/ állatorvos"
      ],
      "nők":[
        "a férjnek szeretője van",
        "mutathatja a pár betegségeit"
      ]
    },
    "7": {
      "általános": [
        "a külvilág felnéz rá",
        "nyilvánosság előtt szerepel",
        "szinte esélytelen normális párkapcsolat",
        "nem működik egyszerre a csilvános szereplés és a párkapcsolat",
        "a párja olyan szerepbe kerül ahol felnéznek rá, megbecsülik",
        "lehet olyan társulásban ahol a társa a főnök",
        "mutathat nyert pereket",
        "külvilág által eért sikerek",
        "külvilágról jövő támogatás",
        "olyan ellenségek akik rangban feletted állnak"
      ],
      "férfi": [
        "nehéz a megélés, mert a párra kell felnéznie"
      ]
    },
    "8": {
      "általános": [
        "le kell mondani a fensőbbségről az uralkodásról",
        "gyerekkorban hozhat apavesztést, szülő vesztést",
        "felerősíti a szexualitást",
        "mutathatja a pár anyagi helyzetét",
        "a pár anyagiakon keresztül teljesedik ki",
        "a pár irányít az anyagi területen",
        "hivatások: bankár, bróker, orvos, sebész, pszichológus, halállal, elmúlással, tudattalannal való foglalkozás",
        "örökség általi gazdagodás",
        "nyereményeket hozhat",
        "pár halála, pár elvesztése, válás",
        "olyan betegségek, amik halálig, vagy műtétig visznek el",
        "híres halál pl újságban vagy tévében megjelenik"
      ]
    },
    "9": {
      "általános": [
        "legkevésbé kérdőjelezi meg sorsát, életét: pozitív",
        "könnyebben fogad el dolgokat",
        "önelfogadás, önbizalom, önértékelés rendben van nála",
        "politikai pályafutás",
        "egyetemi tanulmányok, magasabb tanulmányok",
        "asztrológia, ezotéria spiritualitás ága",
        "vallásban való kiteljesedés",
        "házasság, párkapcsolat, boldog házasság",
        "2. párod lesz akire felnézel",
        "gyereket csak törvényes párkapcsolatban akar",
        "törvénnyel foglalkozó hivatások jöhetnek",
        "sikereket mutathat külföldön",
        "hosszabb külföldi utazások",
        "külföldi tanítványok, vagy tanulás",
        "boldog 2. házasság",
        "fontos az unoka szerepe",
        "az apának fontos a munka",
        "szülő és apa iránti tisztelet",
        "környezetemben a párom irányít"
      ],
      "nő": [
        "fontosabb a párkapcsolat mint a gyerek",
        "nem a család lesz a fontos hanem a pár",
        "erős én tudat: ego"
      ]
    },
    "10": {
      "általános": [
        "az anya irányítani, akar, főnökösködő",
        "harcok, küzdelmek az anyával",
        "elvát szülő ezért az anya az úr a háznál",
        "hivatások: tanítás, nyilvános szereplés, "
      ],
      "nő": [
        "10-es házas nappal nincs karrierje a nőnek, 10 es hold kell hozzá"
      ]
    },
    "11": {
      "általános": [
        "fontosak a  barátok",
        "híres, hírneves barátok",
        "magas beosztású barátok",
        "alapítvány egyesület létrehozását támogatja",
        "társadalomban vezető szerepben lehet",
        "vágyait önmaga teljesíti be",
        "férfi barátok, férfi segítők, az apa segít",
        "az anyának van pénzze",
        "párnak fontos a gyerek, a szerelem, a vállalkozás",
        "a pár gyereke irányít"
      ]
    },
    "12": {
      "általános": [
        "úgy teljesedhet ki, ha mások akrják, hogy kiteljesedjen",
        "titkos kapcsolatokat mutathat",
        "szeretőtől lehet gyereke",
        "nyugdíjasként végzett munka",
        "kórházakban, zárt intézetekben dolgozhat, érhetel sikert",

        "írói alkalmatosság",
        "ezoterikus, spirituális tevékenység",
        "törvénnyle való összeütközés",
        "agy, gerinc, szív, szem, krónikus betegség lehet"
      ]
    }
  },
